"&nbsp;&nbsp;(f&nbsp;x&nbsp;(lambda&nbsp;(v)&nbsp;(k&nbsp;(cons&nbsp;'b&nbsp;"
"v)))))]<br>"
msgstr ""

#: further.html:1545
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;[h&nbsp;(lambda&nbsp;"
"(x&nbsp;k)&nbsp;(g&nbsp;(cons&nbsp;'c&nbsp;x)&nbsp;k))])<br>"
msgstr ""

