msgid "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(begin<br>"
msgstr ""

#: binding.html:789
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(define-syntax&nbsp;var&nbsp;"
"expr)<br>"
msgstr ""

#: binding.html:791
msgid "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;...)]))<br>"
msgstr ""

#: binding.html:793
msgid "(let&nbsp;()<br>"
msgstr ""

#: binding.html:795
msgid "&nbsp;&nbsp;(define&nbsp;plus<br>"
msgstr ""

