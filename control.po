msgstr ""

#: control.html:1944
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;(if&nbsp;(or&nbsp;(null?&nbsp;ls)&nbsp;(null?&nbsp;"
"(cdr&nbsp;ls)))<br>"
msgstr ""

#: control.html:1946
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(values&nbsp;ls&nbsp;'())<br>"
msgstr ""

