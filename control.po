"</p><p>\n"
"</p><p><tt>&nbsp;&nbsp;(define&nbsp;call/cc<br>"
msgstr ""

#: control.html:2177
msgid "&nbsp;&nbsp;&nbsp;&nbsp;(lambda&nbsp;(p)<br>"
msgstr ""

#: control.html:2179
msgctxt "control.html:2179"
msgid "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(rnrs:call/cc<br>"
msgstr ""

#: control.html:2181
msgctxt "control.html:2181"
msgid "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(lambda&nbsp;(k)<br>"
msgstr ""

#: control.html:2183
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;(p&nbsp;"
"(lambda&nbsp;args<br>"
msgstr ""

#: control.html:2188
msgid ""
"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"
"&nbsp;&nbsp;&nbsp;(k&nbsp;(apply&nbsp;values&nbsp;args))))))))\n"
"<br>\n"
"<br>\n"
"&nbsp;&nbsp;(define&nbsp;values<br>"
